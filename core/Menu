                STACK_NAME="wppconnect${opcao2:+_$opcao2}"
                if grep -q "Token: .\+" /root/dados_vps/dados_portainer; then
                    ferramenta_wppconnect "$opcao2"
                else
                    APP_ORION="ferramenta_wppconnect"
                    ferramenta_wppconnect
                fi
                ## FIM TOKEN
            fi   
            ;;

